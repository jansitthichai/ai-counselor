{
  "name": "myproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true npm run build && npm run start",
    "bundle-report": "npm run build && npx @next/bundle-analyzer .next/static/chunks",
    "optimize": "npm run lint:fix && npm run type-check && npm run build",
    "clean": "rm -rf .next out",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-api": "node setup-api.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@vercel/kv": "^3.0.0",
    "chart.js": "^4.4.9",
    "critters": "^0.0.23",
    "framer-motion": "^12.18.1",
    "mongodb": "^6.18.0",
    "next": "^14.2.31",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
